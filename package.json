{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter kit",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "cz": "git-cz",
    "dev": "cross-env NODE_ENV=local nodemon",
    "lint": "tslint \"./src/**/*.ts\" \"./src/**/*.tsx\"",
    "precommit": "lint-staged",
    "start": "node dist/main.js",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "test:watch": "cross-env NODE_ENV=test nyc mocha --watch --recursive"
  },
  "dependencies": {
    "@nestjs/common": "^4.5.9",
    "@nestjs/core": "^4.5.10",
    "@nestjs/microservices": "^4.5.8",
    "@nestjs/swagger": "^1.1.4",
    "@nestjs/testing": "^4.5.5",
    "@nestjs/typeorm": "^2.0.0",
    "@nestjs/websockets": "^4.5.8",
    "autobind-decorator": "^2.1.0",
    "axios": "^0.18.0",
    "class-transformer": "^0.1.9",
    "class-validator": "^0.8.1",
    "clf-date": "^0.1.0",
    "cls-hooked": "^4.2.2",
    "mongodb": "^2.2.33",
    "morgan": "^1.9.0",
    "node-polyglot": "^2.2.2",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "typeorm": "^0.1.12",
    "typescript": "^2.7.2",
    "uuid": "^3.2.1",
    "winston": "^2.4.0",
    "yenv": "^2.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/mock-require": "^2.0.0",
    "@types/nock": "^9.1.2",
    "@types/node": "^9.3.0",
    "@types/node-polyglot": "^0.4.30",
    "@types/sinon": "^4.1.3",
    "@types/winston": "^2.3.8",
    "chai": "^4.1.2",
    "commitizen": "^2.9.6",
    "cross-env": "^5.1.3",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^0.14.3",
    "json-d-ts": "^1.0.1",
    "jsonlint-cli": "^1.0.1",
    "lint-staged": "^7.0.0",
    "markdownlint-cli": "^0.7.1",
    "mocha": "^5.0.1",
    "mocha-typescript": "^1.1.12",
    "nock": "^9.2.1",
    "nodemon": "^1.14.1",
    "nyc": "^11.4.1",
    "prettier": "^1.10.2",
    "sinon": "^4.4.2",
    "source-map-support": "^0.5.3",
    "supertest": "^3.0.0",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.8.0",
    "tslint-plugin-prettier": "^1.3.0",
    "yaml-lint": "^1.1.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "*.json": [
      "jsonlint-cli --sort",
      "git add"
    ],
    ".{yaml,yml}": [
      "yamllint",
      "git add"
    ],
    ".md": [
      "markdownlint",
      "git add"
    ]
  }
}
