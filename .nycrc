{
  "check-coverage": true,
  "lines": 80,
  "statements": 80,
  "functions": 70,
  "branches": 60,
  "include": [
    "src/**/*.ts"
  ],
  "extension": [
    ".ts"
  ],
  "exclude": [
    "src/**/*.test.ts",
    "src/**/*.mock.ts",
    "src/**/*.entity.ts"
  ],
  "require": [
    "ts-node/register"
  ],
  "reporter": [
    "lcov",
    "text-summary",
    "html"
  ],
  "sourceMap": true,
  "instrument": true
}
